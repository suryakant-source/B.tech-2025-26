/*
Q29) WAP to compute roots of a quadratic equation ax^2 + bx + c.
Hint: (-b ± √(b^2-4ac)) / 2a
*/

#include <stdio.h>
#include <math.h>

int main() {
    float a, b, c, d, r1, r2;
    printf("Enter a, b, c: ");
    scanf("%f %f %f", &a, &b, &c);

    d = b*b - 4*a*c;

    if (d < 0) {
        printf("No real roots\n");
    } else if (d == 0) {
        r1 = -b / (2*a);
        printf("One real root: %.2f\n", r1);
    } else {
        r1 = (-b + sqrt(d)) / (2*a);
        r2 = (-b - sqrt(d)) / (2*a);
        printf("Roots: %.2f and %.2f\n", r1, r2);
    }
    return 0;
}
/*
Q30) WAP to enter a three digit number and compute its reverse.
Hint: Enter a number 728 → Reverse = 827
*/

#include <stdio.h>

int main() {
    int num, rev = 0, digit;
    printf("Enter a three-digit number: ");
    scanf("%d", &num);

    while (num > 0) {
        digit = num % 10;
        rev = rev * 10 + digit;
        num /= 10;
    }

    printf("Reverse = %d\n", rev);
    return 0;
}
/*
